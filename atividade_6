#include <stdio.h>

int main (){
	float tempo_gasto;     
    float velocidade_media;  
    float distancia_percorrida; 
    float litros_gasolina;  
    const float rendimento_carro = 12.0;
    printf("Digite o tempo gasto na viagem (em horas): ");
    scanf("%f", &tempo_gasto);
    printf("Digite a velocidade media durante a viagem (em km/h): ");
    scanf("%f", &velocidade_media);
    distancia_percorrida = tempo_gasto * velocidade_media;
    litros_gasolina = distancia_percorrida / rendimento_carro;
    printf("Quantidade de litros de gasolina utilizados: %.2f L\n", litros_gasolina);

    return 0;

}